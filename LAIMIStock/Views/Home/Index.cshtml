@model LAIMIStock.ViewModels.ListCategoriesSViewModel
@{
    ViewBag.Title = "Categorías";
}

<h2>Categorías de Suministros</h2>

<div class="img-grid">
    @foreach (var category in Model.Categories)
    {
        <a href="@Url.Action("ListSupplies", "SupplyCategories", new {id = @category.idCategoriaSuministro})">
            <img src="@category.imagenURL" height="168" width="168" class="img-item">
        </a>
    }
</div>